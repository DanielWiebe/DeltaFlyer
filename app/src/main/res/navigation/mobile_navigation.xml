<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_route">


    <fragment
        android:id="@+id/navigation_route"
        android:name="com.shiftdev.deltaflyer.ui.Route.RouteFragment"
        android:label="@string/title_route"
        tools:layout="@layout/fragment_route_overview" />

    <fragment
        android:id="@+id/navigation_street"
        android:name="com.shiftdev.deltaflyer.ui.Street.StreetFragment"
        android:label="@string/title_street"
        tools:layout="@layout/fragment_street_overview" />

    <fragment
        android:id="@+id/navigation_house"
        android:name="com.shiftdev.deltaflyer.ui.House.HouseFragment"
        android:label="@string/title_house"
        tools:layout="@layout/fragment_house_overview" />
</navigation>
